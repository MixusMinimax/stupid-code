{
    "name": "stupid-code",
    "displayName": "stupid-code",
    "description": "Language Support for Stupid",
    "publisher": "MixusMinimax",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "stupid",
            "aliases": ["Stupid", "stupid"],
            "extensions": [".stupid",".st"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "stupid",
            "scopeName": "source.stupid",
            "path": "./syntaxes/stupid.tmLanguage.json"
        }]
    },
    "repository": "MixusMinimax/stupid-code"
}